<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns="http://rts.dt.com/ladmv/services/inquiries/VehicleInquiry"
	xmlns:jaxb="http://java.sun.com/xml/ns/jaxb" jaxb:version="2.1"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:core="http://rts.dt.com/ladmv/services/inquiries/CoreElements"
	xmlns:tns="http://rts.dt.com/ladmv/services/inquiries/VehicleInquiry"
	targetNamespace="http://rts.dt.com/ladmv/services/inquiries/VehicleInquiry">

	<!-- This annotation helps us in creating proper element type in Stubs (To 
		remove JaxBElementType<String>) -->
	<xsd:annotation>
		<xsd:appinfo>
			<jaxb:globalBindings generateElementProperty="false" />
		</xsd:appinfo>
	</xsd:annotation>

	<xsd:import namespace="http://rts.dt.com/ladmv/services/inquiries/CoreElements"
		schemaLocation="CoreElements.xsd" />

	<xsd:element name="VehicleInquiryRequest">
		<xsd:complexType>
			<!-- <xsd:annotation> <xsd:documentation> Performs a Vehicle Inquiry Using 
				Vin and Plate (VM and LM) and returns the Vehicle results.</xsd:documentation> 
				</xsd:annotation> -->

			<xsd:sequence>
				<xsd:element name="AuthHeader">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element type="xsd:string" name="OfficeCode" />
							<xsd:element type="xsd:string" name="OperatorCode"
								minOccurs="0" maxOccurs="1" nillable="true" />
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>

				<xsd:element name="Inquiry">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="InquiryType" type="xsd:string"
								minOccurs="0" maxOccurs="1" nillable="true" />
							<xsd:element name="Vin" type="xsd:string" minOccurs="0"
								maxOccurs="1" nillable="true" />
							<xsd:element name="PlateNum" type="xsd:string"
								minOccurs="0" maxOccurs="1" nillable="true" />
							<xsd:element name="RegExpYear" minOccurs="0"
								maxOccurs="1" nillable="true">
								<xsd:simpleType>
									<xsd:restriction base="xsd:int">
										<xsd:pattern value="[0-9][0-9]" />
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>

	<xsd:element name="VehicleInquiryResponse">
		<xsd:complexType>
			<!-- <xsd:annotation> <xsd:documentation>Performs a Vehicle Inquiry Using 
				Vin and Plate (VM and LM) and returns the Vehicle results.</xsd:documentation> 
				</xsd:annotation> -->

			<xsd:sequence>
				<xsd:element name="AuthHeader">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element type="xsd:string" name="OfficeCode" />
							<xsd:element type="xsd:string" name="OperatorCode"
								minOccurs="0" maxOccurs="1" nillable="true" />
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="Inquiry">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="BatchNum" type="xsd:long"
								minOccurs="0" maxOccurs="1" nillable="true" />
							<xsd:element name="SequenceNumber" type="xsd:long"
								minOccurs="0" maxOccurs="1" nillable="true" />
							<xsd:element name="PrivacyActCompliance" type="xsd:int"
								minOccurs="0" maxOccurs="1" nillable="true" />
							<xsd:element name="Vehicle" type="tns:VehicleType"
								minOccurs="0" maxOccurs="1" nillable="true" />
							<xsd:element name="Owners" type="tns:OwnersType"
								minOccurs="0" maxOccurs="1" nillable="true" />
							<xsd:element name="Registration" type="tns:RegistrationType"
								minOccurs="0" maxOccurs="1" nillable="true" />
							<xsd:element name="Title" type="tns:TitleType"
								minOccurs="0" maxOccurs="1" nillable="true" />
							<xsd:element name="LienHolder" type="tns:LienHolderType"
								minOccurs="0" maxOccurs="1" nillable="true" />
							<xsd:element name="Sales" type="tns:SalesType"
								minOccurs="0" maxOccurs="1" nillable="true" />
							<xsd:element name="Comments" type="core:DealComments"
								minOccurs="0" maxOccurs="1"/>
							<xsd:element name="Errors" type="core:ErrorsListType"
								minOccurs="0" maxOccurs="1" />
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>

	<xsd:complexType name="VehicleType">
		<xsd:complexContent>
			<xsd:extension base="core:Vehicle">
				<xsd:sequence>
					<xsd:element name="IsNew">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:enumeration value="N" />
								<xsd:enumeration value="U" />
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:element>
					<xsd:element name="OdometerReading" type="xsd:string" />
					<xsd:element name="ClassCode" type="xsd:string"
						minOccurs="0" maxOccurs="1" nillable="true" />
					<xsd:element name="Flags" type="core:FlagListType"
						minOccurs="0" maxOccurs="1" nillable="true" />
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="OwnersType">
		<xsd:sequence>
			<xsd:element name="PrimaryOwner" type="tns:PrimaryOwnerType"
				nillable="true" />
			<xsd:element name="SecondaryOwner" type="tns:SecondaryOwnerType"
				minOccurs="0" maxOccurs="1" nillable="true" />
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="TitleType">
		<xsd:complexContent>
			<xsd:extension base="core:Title">
				<xsd:sequence>
					<xsd:element name="MicroFilmNum" type="xsd:string"
						minOccurs="0" maxOccurs="1" nillable="true" />
					<xsd:element name="PrevNonTitleTransTypeCode" type="xsd:string"
						minOccurs="0" maxOccurs="1" nillable="true" />
					<xsd:element name="PrevNonTitleTransDate" minOccurs="0">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:pattern value="[0-9]{6}"></xsd:pattern>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="RegistrationType">
		<xsd:complexContent>
			<xsd:extension base="core:Registration">
				<xsd:sequence>
					<xsd:element name="PrevPlateNum" type="xsd:string"
						minOccurs="0" maxOccurs="1" nillable="true" />
					<xsd:element name="PrevPlateExpDate" minOccurs="0"
						maxOccurs="1" nillable="true">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:pattern value="[0-9]{6}"></xsd:pattern>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="PrimaryOwnerType">
		<xsd:complexContent>
			<xsd:extension base="core:Party">
				<xsd:sequence>
					<xsd:element name="OwnerType" type="xsd:int" minOccurs="0"
						maxOccurs="1" nillable="true" />
					<xsd:element name="JointOwnerName" type="xsd:string"
						minOccurs="0" maxOccurs="1" nillable="true" />
					<xsd:element name="DriverLicenseNum" type="xsd:string"
						minOccurs="0" maxOccurs="1" nillable="true" />
					<xsd:element name="EmpIdentificationNum" type="xsd:string"
						minOccurs="0" maxOccurs="1" nillable="true" />
					<xsd:element name="CustomerNum" type="xsd:string"
						minOccurs="0" maxOccurs="1" nillable="true" />
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="SecondaryOwnerType">
		<xsd:complexContent>
			<xsd:extension base="core:Party">
				<xsd:sequence>
					<xsd:element name="OwnerType" type="xsd:int" minOccurs="0"
						maxOccurs="1" nillable="true" />
					<xsd:element name="OwnerStatus" type="xsd:string"
						minOccurs="0" maxOccurs="1" nillable="true" />
					<xsd:element name="CustomerNum" type="xsd:string"
						minOccurs="0" maxOccurs="1" nillable="true" />
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="LienHolderType">
		<xsd:complexContent>
			<xsd:extension base="core:Party">
				<xsd:sequence>
					<xsd:element name="LienDate">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:pattern value="[0-9]{6}"></xsd:pattern>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:element>
					<xsd:element name="LienAmount" minOccurs="0" maxOccurs="1"
						nillable="true">
						<xsd:simpleType>
							<xsd:restriction base="xsd:decimal">
								<xsd:fractionDigits value="2" />
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="SalesType">
		<xsd:sequence>
			<xsd:element name="DateAcquired">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:pattern value="[0-9]{6}"></xsd:pattern>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="SalePrice">
				<xsd:simpleType>
					<xsd:restriction base="xsd:decimal">
						<xsd:fractionDigits value="2" />
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="SpecialFeesCode" type="xsd:string" />
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="CommentsType">
		<xsd:sequence>
			<xsd:element name="Comments" type="core:DealComments"
				minOccurs="0" maxOccurs="1" nillable="true" />
		</xsd:sequence>
	</xsd:complexType>

</xsd:schema>